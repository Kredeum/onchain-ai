{
  "name": "@onchain-ai/svelte5",
  "private": true,
  "type": "module",
  "version": "1.0.11",
  "author": "Kredeum",
  "description": "OnChainAI Svelte5 package",
  "scripts": {
    "build": "vite build",
    "check": "pnpm run lint && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "clean": "rm -rf node_modules .turbo build .svelte-kit",
    "fleek": "pnpm run build && fleek sites deploy",
    "dev": "vite dev",
    "fleek:login": "fleek login",
    "format": "prettier --write --list-different .",
    "lint": "prettier --check . && eslint .",
    "preview": "vite preview",
    "start": "vite dev",
    "vercel": "vercel",
    "e2e:chromium": "playwright install chromium",
    "e2e": "playwright test",
    "e2e:headed": "pnpm e2e --headed",
    "e2e:debug": "pnpm e2e --debug",
    "e2e:ui": "pnpm e2e --ui",
    "e2e:report": "playwright show-report"
  },
  "devDependencies": {
    "@castlenine/svelte-qrcode": "^2.3.0",
    "@coinbase/wallet-sdk": "^4.2.4",
    "@fleek-platform/cli": "^3.8.2",
    "@onchain-ai/common": "workspace:^",
    "@onchain-ai/foundry": "workspace:^",
    "@playwright/test": "^1.49.1",
    "@sveltejs/adapter-static": "^3.0.6",
    "@sveltejs/kit": "^2.11.1",
    "@sveltejs/vite-plugin-svelte": "^4.0.3",
    "@tanstack/svelte-query": "^5.62.7",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "@uniswap/sdk-core": "^6.1.0",
    "@uniswap/v2-sdk": "^4.7.0",
    "@wagmi/connectors": "^5.6.1",
    "@wagmi/core": "^2.16.0",
    "abitype": "^1.0.7",
    "autoprefixer": "^10.4.20",
    "blo": "^1.2.0",
    "cross-fetch": "^4.0.0",
    "daisyui": "^4.12.22",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.46.1",
    "globals": "^15.13.0",
    "playwright-core": "^1.49.1",
    "postcss": "^8.4.49",
    "postcss-load-config": "^6.0.1",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "svelte": "^5.14.0",
    "svelte-check": "^4.1.1",
    "svelte-hero-icons": "^5.2.0",
    "svelte-hot-french-toast": "^1.0.0",
    "tailwindcss": "^3.4.16",
    "tslib": "^2.8.1",
    "type-fest": "^4.30.1",
    "typescript": "^5.7.2",
    "viem": "^2.21.55",
    "vite": "^5.4.11",
    "vitest": "^2.1.8"
  }
}
